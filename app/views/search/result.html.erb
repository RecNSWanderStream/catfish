<div class="results">
<h1>Listing assets</h1>

<table>
  <thead>
    <tr>
      <th>Asset type</th>
      <th>Profile</th>
      <th>Region</th>
      <th>Name</th>
      <th>Description</th>
      <th>Lat</th>
      <th>Lng</th>
      <th>Created by</th>
      <th>Updated by</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @results.each do |result| %>
      <tr>
        <td name="asset_type_id"><%= result.asset_type_id %></td>
        <td name="profile_id"><%= result.profile_id %></td>
        <td name="region_id"><%= result.region_id %></td>
        <td name="name"><%= result.name %></td>
        <td name="description"><%= result.description %></td>
        <td name="lat"><%= result.lat %></td>
        <td name="lng"><%= result.lng %></td>
        <td name="created_by"><%= result.created_by %></td>
        <td name="updated_by"><%= result.updated_by %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
</div>
<div id="map-canvas"></div>
